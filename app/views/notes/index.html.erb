<h1>Listing Notes</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td><%= note.body %></td>
        <td><%= link_to 'Show', users_note_path(note) %></td>
        <td><%= link_to 'Edit', edit_users_note_path(note) %></td>
        <td><%= link_to 'Destroy', users_note_path(note), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
