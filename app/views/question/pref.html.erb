<%= render layout: 'form' do |f| %>
  <h2><%= f.label(:pref_code, "ご両親のお住まいは？", class: "control-label") %></h2>
  <p>
    <%= f.collection_select(:pref_code, JpPrefecture::Prefecture.all, :code, :name, {},
      class: "form-control input-lg", style: "display: inline-block; width: auto") %>
  </p>
<% end %>
